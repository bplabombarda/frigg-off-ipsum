{
  "bail": true,
  "collectCoverage": true,
  "coverageDirectory": "<rootDir>/logs/jest",
  "coverageReporters": [
    "html",
    "json",
    "lcov",
    "text"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json"
  ],
  "moduleNameMapper": {
    "\\.(css|sass|scss)$": "<rootDir>/src/styles/__mocks__/styleMock.js"
  },
  "modulePathIgnorePatterns": ["public"],
  "rootDir": "../..",
  "setupFiles": [
    "<rootDir>/config/testing/setUp.js"
  ],
  "verbose": true
}
